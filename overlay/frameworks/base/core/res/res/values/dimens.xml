<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>
    <!-- Default radius of the software rounded corners. -->
    <dimen name="rounded_corner_radius">20dp</dimen>

    <!-- Default paddings for content around the corners. -->
    <dimen name="rounded_corner_content_padding">8dp</dimen>

    <!-- The default height of the status bar used in {@link SystemBarUtils#getStatusBarHeight} to calculate the status bar height. -->
    <dimen name="status_bar_height_default">30dp</dimen>
</resources>
